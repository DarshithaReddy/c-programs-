#include <stdio.h> 
int main() {
 int n, rev = 0, remainder, s; 
printf("Enter num: ");
scanf("%d", &n); 
s= n; 
while (n != 0) 
{ 
remainder = n % 10; 
rev = rev* 10 + remainder; 
n /= 10; }
 if (s == rev) 
printf("%d is a palindrome.", s); 
else 
printf("%d is not a palindrome.", s); 
return 0; 
}
